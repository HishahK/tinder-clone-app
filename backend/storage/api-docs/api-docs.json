    "openapi": "3.0.0",
    "info": {
        "title": "Tinder Clone API",
        "description": "API Documentation for the Tinder Clone App",
        "version": "1.0.0"
    },
    "paths": {
        "/api/recommendations": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get user recommendations",
                "operationId": "415dc4f3a5b6c9c31d8bd028164bdd01",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/swipe": {
            "post": {
                "tags": [
                    "Actions"
                ],
                "summary": "Perform a swipe action (like/nope)",
                "operationId": "3bc958f68c76f4a118620a80e18a0a53",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "swiped_id",
                                    "action"
                                ],
                                "properties": {
                                    "swiped_id": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "action": {
                                        "type": "string",
                                        "enum": [
                                            "like",
                                            "nope"
                                        ],
                                        "example": "like"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Action recorded"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/likes": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get list of liked users",
                "operationId": "32a4e3b6ba0c528b34220da16a60eb8f",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        }
    },
